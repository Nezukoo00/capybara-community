{
  "name": "capybara-community",
  "version": "1.0.0",
  "description": "Полнофункциональное веб-сообщество любителей капибар",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server . -p 8000",
    "serve": "npx http-server . -p 8000 -o",
    "deploy": "netlify deploy --prod",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "netlify dev:exec drizzle-kit migrate",
    "db:studio": "netlify dev:exec drizzle-kit studio"
  },
  "keywords": [
    "capybara",
    "community",
    "social",
    "website"
  ],
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "drizzle-kit": "^0.31.7",
    "http-server": "^14.1.1",
    "netlify-cli": "^23.12.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "postgres": "^3.4.7"
  }
}
